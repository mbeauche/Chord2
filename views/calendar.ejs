<form name="add" action="/add" method="post" onsubmit="return validateCreate()">

	<input type="text" name="username" placeholder="username" /> </br>

	<input type="text" name="roomtype" placeholder="room type" /></br>

	<input type="text" name="location" placeholder="room location" /></br>

	<input type="text" name="starttime" placeholder="start time" /></br>

	<input type="text" name="endtime" placeholder="end time" /></br>

	<input type="text" name="startdate" placeholder="start date" /></br>

	<input type="text" name="enddate" placeholder="end date" /></br>

	<input type="text" name="inswap" placeholder="in swap?" /></br>

	<input type="text" name="swapnote" placeholder="swap note" /></br>

	<input type="submit" value="ADD IT ALL">
</form> 

<form name="register" action="/validateregister" method="post" onsubmit="return validateCreate()">
