	<input type="password" name="password2" placeholder="Confirm Password" /></br>
	<input type="text" name="fullname" placeholder="Full Name" /></br>
	<input type="text" name="major" placeholder="Major" /></br>
	<input type="submit" value="Sign up">
</form> 
